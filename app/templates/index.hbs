<div class="container">
  <div class="row">
    <div class="col-sm-8 col-sm-offset-1">
      <ul>
        {{#each model.questions as |question|}}
          {{question-tile question=question}}
        {{/each}}
      </ul>
    </div>
    <div class="col-sm-3 tags">
      {{by-tags tags=model.tags}}
    </div>
  </div>

  <div class="row">
    <div class="col-sm-8 col-sm-offset-2">
      {{new-question save2="save3"}}
    </div>
  </div>
</div>
